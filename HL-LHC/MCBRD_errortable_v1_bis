MAX_MCBRDH = BRef_MCBRDH*l.MCBRDH*CLIGHT/(1E9*NRJ);
MAX_MCBRDV = BRef_MCBRDV*l.MCBRDV*CLIGHT/(1E9*NRJ);
threshold=0.1;
threshold_include_sign=0;

exec assign_sign_from_optics(sign_MCBRDH.4L1.B1,acbrdh4.l1b1,MAX_MCBRDH,threshold,threshold_include_sign);
exec assign_sign_from_optics(sign_MCBRDH.4R1.B1,acbrdh4.r1b1,MAX_MCBRDH,threshold,threshold_include_sign);
exec assign_sign_from_optics(sign_MCBRDH.4L5.B1,acbrdh4.l5b1,MAX_MCBRDH,threshold,threshold_include_sign);
exec assign_sign_from_optics(sign_MCBRDH.4R5.B1,acbrdh4.r5b1,MAX_MCBRDH,threshold,threshold_include_sign);
exec assign_sign_from_optics(sign_MCBRDV.4L1.B1,acbrdv4.l1b1,MAX_MCBRDV,threshold,threshold_include_sign);
exec assign_sign_from_optics(sign_MCBRDV.4R1.B1,acbrdv4.r1b1,MAX_MCBRDV,threshold,threshold_include_sign);
exec assign_sign_from_optics(sign_MCBRDV.4L5.B1,acbrdv4.l5b1,MAX_MCBRDV,threshold,threshold_include_sign);
exec assign_sign_from_optics(sign_MCBRDV.4R5.B1,acbrdv4.r5b1,MAX_MCBRDV,threshold,threshold_include_sign);
exec assign_sign_from_optics(sign_MCBRDH.4L1.B2,acbrdh4.l1b2,MAX_MCBRDH,threshold,threshold_include_sign);
exec assign_sign_from_optics(sign_MCBRDH.4R1.B2,acbrdh4.r1b2,MAX_MCBRDH,threshold,threshold_include_sign);
exec assign_sign_from_optics(sign_MCBRDH.4L5.B2,acbrdh4.l5b2,MAX_MCBRDH,threshold,threshold_include_sign);
exec assign_sign_from_optics(sign_MCBRDH.4R5.B2,acbrdh4.r5b2,MAX_MCBRDH,threshold,threshold_include_sign);
exec assign_sign_from_optics(sign_MCBRDV.4L1.B2,acbrdv4.l1b2,MAX_MCBRDV,threshold,threshold_include_sign);
exec assign_sign_from_optics(sign_MCBRDV.4R1.B2,acbrdv4.r1b2,MAX_MCBRDV,threshold,threshold_include_sign);
exec assign_sign_from_optics(sign_MCBRDV.4L5.B2,acbrdv4.l5b2,MAX_MCBRDV,threshold,threshold_include_sign);
exec assign_sign_from_optics(sign_MCBRDV.4R5.B2,acbrdv4.r5b2,MAX_MCBRDV,threshold,threshold_include_sign);

value,sign_MCBRDH.4L1.B1,sign_MCBRDH.4R1.B1,sign_MCBRDH.4L5.B1,sign_MCBRDH.4R5.B1,sign_MCBRDV.4L1.B1,sign_MCBRDV.4R1.B1,sign_MCBRDV.4L5.B1,sign_MCBRDV.4R5.B1,sign_MCBRDH.4L1.B2,sign_MCBRDH.4R1.B2,sign_MCBRDH.4L5.B2,sign_MCBRDH.4R5.B2,sign_MCBRDV.4L1.B2,sign_MCBRDV.4R1.B2,sign_MCBRDV.4L5.B2,sign_MCBRDV.4R5.B2;

return;
